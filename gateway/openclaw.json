{
  "agents": {
    "defaults": {
      "workspace": "/app/workspace"
    }
  },
  "gateway": {
    "bind": "lan",
    "trustedProxies": ["100.64.0.1", "100.64.0.2", "100.64.0.3", "100.64.0.4", "100.64.0.5", "100.64.0.6", "100.64.0.7", "100.64.0.8", "100.64.0.9", "100.64.0.10", "100.64.0.11", "100.64.0.12"],
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "controlUi": {
      "allowedOrigins": ["https://gateway-production-1382.up.railway.app"]
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "botToken": "${TELEGRAM_BOT_TOKEN}",
      "dmPolicy": "allowlist",
      "allowFrom": ["tg:7664703896"],
      "actions": { "sendMessage": true },
      "groups": {
        "*": { "requireMention": true }
      }
    }
  },
  "plugins": {
    "enabled": true,
    "allow": ["work-agent", "telegram"],
    "load": {
      "paths": ["/app/work-agent-plugin"]
    },
    "entries": {
      "telegram": {
        "enabled": true
      },
      "work-agent": {
        "enabled": true,
        "config": {}
      }
    }
  }
}
